    "eslint-plugin-react": "^6.2.0",
    "express": "^4.14.0",
    "pg": "^6.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0",
    "webpack": "^1.13.2",
